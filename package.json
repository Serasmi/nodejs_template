  },
  "license": "ISC",
  "name": "nodejs-template",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "cross-env NODE_ENV=production cross-env BABEL_ENV=server babel src -d dist --copy-files",
    "dev": "cross-env NODE_ENV=development cross-env BABEL_ENV=server nodemon src --exec babel-node --watch src --watch config -e js,sql,json",
    "start": "node dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "morgan": "^1.10.0",
    "pg-promise": "^10.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.12.1",
