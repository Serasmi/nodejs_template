{
  "name": "nodejs-template",
  "author": "Serasmi",
  "description": "Node.js template",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "build": "cross-env NODE_ENV=production cross-env BABEL_ENV=server babel src -d dist --ignore 'src/register.ts' --extensions '.js,.json,.ts'",
    "start:dev": "cross-env NODE_ENV=development cross-env BABEL_ENV=server nodemon --exec babel-node src/register.ts --exec babel-node --watch src --watch config -e js,sql,json,ts",
    "start:prod": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/runtime": "7.17.8",
    "@burdzin/coded-error": "1.2.0",
    "@types/mongodb": "4.0.7",
    "body-parser": "1.19.2",
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "ip": "1.1.5",
    "joi": "17.6.0",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "pg-promise": "10.11.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.7",
    "@babel/node": "7.16.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@babel/register": "7.17.7",
    "@types/config": "0.0.36",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/ip": "1.1.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/morgan": "1.9.3",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.12.0",
    "eslint-import-resolver-typescript": "2.7.0",
    "eslint-plugin-import": "2.25.4",
    "nodemon": "2.0.15",
    "prettier": "2.6.1",
    "typescript": "4.6.2"
  }
}
